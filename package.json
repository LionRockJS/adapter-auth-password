{
  "type": "module",
  "name": "@lionrockjs/adapter-auth-password",
  "version": "0.0.6",
  "description": "The Sign In with password Module for LionRockJS",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ../../../node_modules/jest/bin/jest.js --coverage"
  },
  "jest": {
    "verbose": true,
    "transform": {},
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/?(*.)+(spec|test).mjs"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LionRockJS/adapter-auth.git"
  },
  "keywords": [
    "KohanaJS"
  ],
  "author": "Kojin Nakana",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LionRockJS/adapter-auth/issues"
  },
  "homepage": "https://github.com/LionRockJS/adapter-auth#readme",
  "dependencies": {
    "argon2": "^0.41.1"
  }
}
